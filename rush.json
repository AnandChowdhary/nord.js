{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.70.0",
  "pnpmVersion": "6.7.1",
  "pnpmOptions": {
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.13.0",
  "gitPolicy": {
    "versionBumpCommitMessage": "chore(pkg): Bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "chore(docs): Update changelogs [skip ci]"
  },
  "repository": {
    "url": "https://github.com/AnandChowdhary/nord.js"
  },
  "telemetryEnabled": true,
  "projects": [
    {
      "packageName": "@nordjs/core",
      "projectFolder": "packages/core",
      "shouldPublish": true
    },
    {
      "packageName": "@nordjs/types",
      "projectFolder": "packages/types",
      "shouldPublish": true
    },
    {
      "packageName": "@nordjs/errors",
      "projectFolder": "packages/errors",
      "shouldPublish": true
    },
    {
      "packageName": "@nordjs/cli",
      "projectFolder": "packages/cli",
      "shouldPublish": true
    },
    {
      "packageName": "@nordjs/validator",
      "projectFolder": "packages/validator",
      "shouldPublish": true
    },
    {
      "packageName": "@nordjs/development-node-express",
      "projectFolder": "packages/development-node-express"
    }
  ]
}
